generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  schemas  = ["booking"]
}

model Room {
  id        Int      @id @default(autoincrement())
  name      String
  capacity  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("room")
  @@schema("booking")
}
